{
  "version": 2,
  "builds": [
    {
      "src": "app_vercel.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app_vercel.py"
    }
  ],
  "env": {
    "DISABLE_XFORMERS": "1"
  },
  "functions": {
    "app_vercel.py": {
      "maxDuration": 60
    }
  }
}
